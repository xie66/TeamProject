<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gezhi.gege">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_gege"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_gege"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gege">
        <activity android:name=".RegistActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".news.BrowseNewsActivity" />
        <activity android:name=".news.MainNewsActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>